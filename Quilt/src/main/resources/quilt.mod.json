{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.hidoni",
    "id": "customizableelytra",
    "version": "${version}",
    "metadata": {
      "name": "Customizable Elytra",
      "description": "Adds a variety of colorful ways to customize your elytra!",
      "contributors": {
        "Hidoni": "Owner"
      },
      "contact": {
        "sources": "https://github.com/Hidoni/CustomizableElytra"
      },
      "license": "MIT",
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.hidoni.customizableelytra.CustomizableElytraQuilt"
      ],
      "client_init": [
        "com.hidoni.customizableelytra.CustomizableElytraQuiltClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.19.0"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.0.1-"
      },
      {
        "id": "minecraft",
        "versions": [
          "1.20",
          "1.20.1"
        ]
      }
    ]
  },
  "mixin": [
    "mixins.customizableelytra.common.json",
    "mixins.customizableelytra.quilt.json"
  ]
}